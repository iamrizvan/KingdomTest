<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="work.rizvan.com.kingdomtest">

    <application
        android:allowBackup="true"
        android:icon="@drawable/todo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/todo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.ToDoActivity"
            android:label="@string/title_activity_todo"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.RegistrationActivity" />
        <activity android:name=".activity.CreateToDoActivity" />
        <activity android:name=".activity.ToDoEditActivity" />
        <activity android:name=".activity.ToDoDescriptionActivity"></activity>
    </application>

</manifest>